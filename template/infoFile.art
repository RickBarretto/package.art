infoFile: $[data :dictionary][
    library?: data\library?
    executable?: data\executable?

    join.lines @[
        if library? -> "entry: {src/lib}"
        if executable? -> "executable: {src/main}"
        "depends: ["
        indent "[unitt >= 3.1.1]"
        "]"
        "requires: [>= 0.10.0]"
    ]
]